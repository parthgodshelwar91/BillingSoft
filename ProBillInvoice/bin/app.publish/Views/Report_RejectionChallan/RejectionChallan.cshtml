@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <script>
        $(document).ready(function () {
            var selectedId = localStorage.getItem('selectedRCId');
            if (selectedId) {
                $('.list-group-item').each(function () { //already Class code
                    if ($(this).attr('href').includes('RCId=' + selectedId)) {
                        $(this).addClass('selected');
                    }
                });
            }

            $('.list-group-item').on('click', function (e) {
                var href = $(this).attr('href');
                var liId = new URLSearchParams(href.split('?')[1]).get('RCId');
                localStorage.setItem('selectedRCId', liId);
                $('.list-group-item').removeClass('selected');
                $(this).addClass('selected');
            });
        });
    </script>

    <script>
        function AjFunction(GSTStatus) {
            if (GSTStatus === "Within State Supplier") {
                $('#RejectionChallan tr th[colspan]:nth-child(6)').show();
                $('#RejectionChallan tr th[id="cgst"]:nth-child(1)').show();
                $('#RejectionChallan tr th[ id="cgstamt"]:nth-child(2)').show();
                $('#RejectionChallan td:nth-child(6)').show();
                $('#RejectionChallan td:nth-child(7)').show();

                $('#RejectionChallan tr th[colspan]:nth-child(7)').show();
                $('#RejectionChallan tr th[id="sgst"]:nth-child(3)').show();
                $('#RejectionChallan tr th[ id="sgstamt"]:nth-child(4)').show();
                $('#RejectionChallan td:nth-child(8)').show();
                $('#RejectionChallan td:nth-child(9)').show();

                $('#RejectionChallan tr th[colspan]:nth-child(8)').hide();
                $('#RejectionChallan tr th[id="igst"]:nth-child(5)').hide();
                $('#RejectionChallan tr th[ id="igstamt"]:nth-child(6)').hide();
                $('#RejectionChallan td:nth-child(10)').hide();
                $('#RejectionChallan td:nth-child(11)').hide();

            } else if (GSTStatus === "Out of State Supplier") {
                $('#RejectionChallan tr th[colspan]:nth-child(6)').hide();
                $('#RejectionChallan tr th[id="cgst"]:nth-child(1)').hide();
                $('#RejectionChallan tr th[ id="cgstamt"]:nth-child(2)').hide();
                $('#RejectionChallan td:nth-child(6)').hide();
